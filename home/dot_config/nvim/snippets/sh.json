{
  "header": {
    "prefix": "header",
    "body": [
      "#!${1:/bin/bash}",
      "#",
      "# Usage: ${2:filename.sh [flags] <command>}",
      "#",
      "# ${3:Description}",
      "#",
      "",
      "set -eu${0:}"
    ]
  },
  "shellcheck disable": {
    "prefix": "shellcheck-disable",
    "body": "# shellcheck disable=SC${1:}"
  }
}

